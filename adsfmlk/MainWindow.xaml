<Window
    x:Class="adsfmlk.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:adsfmlk"
    xmlns:muxc="using:Microsoft.UI.Xaml.Controls">

    <Grid>
        <muxc:NavigationView SelectionChanged="NavigationView_SelectionChanged">
            <muxc:NavigationView.MenuItems>
                <muxc:NavigationViewItem Content="Home" Tag="HomePage" Icon="Home"/>
                <muxc:NavigationViewItem Content="Wpisy" Tag="Wpisy" Icon="List"/>
                <muxc:NavigationViewItem Content="Recepty" Tag="Recepty" Icon="Page"/>
                <muxc:NavigationViewItem Content="Skierowania" Tag="Skierowania" Icon="TouchPointer"/>
                <muxc:NavigationViewItem Content="Wyniki" Tag="Wyniki" />
            </muxc:NavigationView.MenuItems>
            <Frame x:Name="contentFrame" />
        </muxc:NavigationView>
    </Grid>
</Window>